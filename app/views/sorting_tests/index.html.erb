<section id="main-content">
  <section class="wrapper">
      <div class="row centered">
          <div class="col-lg-12">
            <h1>Card Sorting</h1>
		        <h3>Test your users using card sorting.</h3>
            <%= link_to "Start Test", confirm_test_path(:url => current_project.sorting_test.url), :target => "_blank", 'data-no-turbolink' => true, :class => "btn btn-default btn-success"  %>
            <%= link_to("Image Gallery", project_card_images_path, 'data-no-turbolink' => true, :class => "btn btn-default btn-success" )  %>
          </div>
      </div>
      <div class="row centered">
      </div>
      <div class="row">
      	<%= render "form_new_card" %>
        <div class="col-md-4 col-sm-offset-5">
            <%= render "form_update_password" %>
          </div>
      </div>
      <div class="wrapper">
        <div class="row">
          <%= render "list_cards" %>
        </div>
      </div>
  </section>
</section>