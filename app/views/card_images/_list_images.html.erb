<% current_project.card_images.each do |image| %>
	<div class="col-md-6">
	<%= bootstrap_form_for(image, inline_errors: false, :html => {:class => 'form-card-image'}, :method => :put) do |f| %>
			<div class="login-wrap">
				<div class="centered">
					<%= image_tag image.image.url(:medium) %>
				</div>
				<%= f.file_field :image %>
				<button class="btn btn-theme btn-block" type="submit">Update</button>
			</div>
		<% end %>
	</div>
<% end %>